{
  "name": "journeylog-fe",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.1.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-pro-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.19",
    "axios": "^0.18.0",
    "bbcode-to-react": "^0.2.9",
    "bootstrap": "^4.1.3",
    "chokidar-cli": "^1.2.1",
    "classnames": "^2.2.6",
    "color": "^3.1.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.3.0",
    "local-storage-fallback": "^4.1.1",
    "luxon": "^1.4.4",
    "mobx": "^4.5.1",
    "mobx-react": "^5.3.4",
    "mobx-state-tree": "^2.2.0",
    "modernizr": "^3.6.0",
    "node-sass-chokidar": "^1.3.4",
    "node-sass-glob-importer": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-app-rewire-define-plugin": "^1.0.0",
    "react-app-rewire-mobx": "^1.0.9",
    "react-app-rewire-modernizr": "^0.2.1",
    "react-app-rewired": "^1.6.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ --importer node_modules/node-sass-glob-importer/dist/cli.js -o src/",
    "watch-css": "npm run build-css && chokidar \"src/**/*.scss\" -c \"npm run build-css\"",
    "start-js": "react-app-rewired start",
    "build-js": "react-app-rewired build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always"
  },
  "homepage": "/journeylog/"
}
