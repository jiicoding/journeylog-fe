{
  "name": "journeylog-fe",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bbcode-to-react": "^0.2.9",
    "bootstrap": "^4.1.0",
    "chokidar-cli": "^1.2.0",
    "classnames": "^2.2.5",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "luxon": "^1.2.0",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^2.0.4",
    "node-sass-chokidar": "^1.2.2",
    "node-sass-glob-importer": "^5.1.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ --importer node_modules/node-sass-glob-importer/dist/cli.js -o src/",
    "watch-css": "npm run build-css && chokidar \"src/**/*.scss\" -c \"npm run build-css\"",
    "start-js": "react-app-rewired start",
    "build-js": "react-app-rewired build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always"
  }
}
